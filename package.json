{
  "name": "n8n-nodes-solana-swap",
  "version": "1.6.2",
  "description": "n8n node for Solana blockchain operations with Raydium and Jupiter swap integration. SPL token transfers now fully implemented with @solana/spl-token. Developed by CHECKHC.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "solana",
    "blockchain",
    "crypto",
    "trading",
    "jupiter",
    "raydium",
    "swap",
    "dex",
    "defi"
  ],
  "license": "MIT",
  "homepage": "https://github.com/checkhc/n8n-nodes-solana-swap#readme",
  "author": {
    "name": "CHECKHC",
    "email": "contact@checkhc.net",
    "url": "https://checkhc.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/checkhc/n8n-nodes-solana-swap.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SolanaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SolanaNode/SolanaNode.node.js"
    ]
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.0",
    "bs58": "^5.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "gulp": "^5.0.1",
    "n8n-workflow": "^1.112.0",
    "typescript": "^5.9.3"
  }
}
